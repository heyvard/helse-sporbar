{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Utbetalinger som blir gjort i Spleis",
  "description": "Utbetalinger som er gjort i Spleis\n\nTopic: tbd.utbetaling\n\nKey: Fødselsnummer\n\nHeader[type]: Utbetaling/UtenUtbetaling",
  "required": [
    "event",
    "utbetalingId",
    "korrelasjonsId",
    "fødselsnummer",
    "aktørId",
    "organisasjonsnummer",
    "tom",
    "fom",
    "forbrukteSykedager",
    "gjenståendeSykedager",
    "stønadsdager",
    "automatiskBehandling",
    "type",
    "foreløpigBeregnetSluttPåSykepenger",
    "utbetalingsdager"
  ],
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "event": {
      "type": "string",
      "enum": [
        "utbetaling_utbetalt",
        "utbetaling_uten_utbetaling"
      ]
    },
    "utbetalingId": {
      "type": "string",
      "format": "uuid",
      "examples": ["446eca54-befd-4851-acc3-ec300a20932a"],
      "description": "NB, flere utbetalingId-er kan peke på samme fagsystemId."
    },
    "korrelasjonsId": {
      "type": "string",
      "format": "uuid",
      "examples": ["a43696c7-e824-4140-b8a7-348efe7128cc"]
    },
    "fødselsnummer": {
      "type": "string"
    },
    "aktørId": {
      "type": "string"
    },
    "organisasjonsnummer": {
      "type": "string",
      "examples": ["999263550"]
    },
    "fom": {
      "type": "string",
      "format": "date",
      "examples": ["2018-01-01"]
    },
    "tom": {
      "type": "string",
      "format": "date",
      "examples": ["2018-01-31"]
    },
    "forbrukteSykedager": {
      "type": "integer",
      "description": "Hvor mange virkesykedager er forbrukt totalt.",
      "examples": [70]
    },
    "gjenståendeSykedager": {
      "type": "integer",
      "description": "Hvor mange sykedager det er igjen til maksdato.",
      "examples": [178]
    },
    "stønadsdager": {
      "type": "integer",
      "examples": [70]
    },
    "automatiskBehandling": {
      "type": "boolean",
      "description": "Ble utbetalingen utført automatisk?"
    },
    "type": {
      "type": "string",
      "enum": [
        "UTBETALING",
        "ETTERUTBETALING",
        "ANNULLERING",
        "REVURDERING"
      ]
    },
    "antallVedtak": {
      "type": ["integer", "null"],
      "description": "Antall vedtak som ligger til grunn for utbetalingen.",
      "examples": [1]
    },
    "foreløpigBeregnetSluttPåSykepenger": {
      "type": "string",
      "format": "date",
      "examples": ["2018-12-31"]
    },
    "utbetalingsdager": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "dato",
          "type",
          "begrunnelser"
        ],
        "properties": {
          "dato": {
            "type": "string",
            "format": "date",
            "examples": ["2018-01-01"]
          },
          "begrunnelser": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "SykepengedagerOppbrukt",
                "SykepengedagerOppbruktOver67",
                "MinimumInntekt",
                "MinimumInntektOver67",
                "EgenmeldingUtenforArbeidsgiverperiode",
                "MinimumSykdomsgrad",
                "EtterDødsdato",
                "ManglerOpptjening",
                "ManglerMedlemskap",
                "Over70"
              ]
            }
          },
          "type": {
            "type": "string",
            "description": "AvvistDag: Vil ha innhold i begrunnelser for hvorfor den er avvist\n\nUkjentDag: Vi har ikke mottatt informasjon om denne dagen, så den regnes som en arbeidsdag",
            "enum": [
              "AvvistDag",
              "NavHelgDag",
              "ArbeidsgiverperiodeDag",
              "Arbeidsdag",
              "Fridag",
              "Feriedag",
              "Permisjonsdag",
              "NavDag",
              "ForeldetDag",
              "UkjentDag"
            ]
          }
        }
      }
    },
    "arbeidsgiverOppdrag": {
      "type": ["object", "null"],
      "additionalProperties": false,
      "description": "Optional. Oppdraget som gjelder utbetaling til arbeidsgiver.",
      "required": [
        "mottaker",
        "fagområde",
        "fagsystemId",
        "nettoBeløp",
        "stønadsdager",
        "fom",
        "tom",
        "utbetalingslinjer"
      ],
      "properties": {
        "mottaker": {
          "type": "string",
          "description": "Arbeidsgiver",
          "examples": ["999263550"]
        },
        "fagområde": {
          "type": "string",
          "enum": ["SPREF"]
        },
        "fagsystemId": {
          "type": "string",
          "examples": ["XI2MMEZAJZBVJL2E4K7UM4BQBY"]
        },
        "nettoBeløp": {
          "type": "integer",
          "examples": [7155]
        },
        "stønadsdager": {
          "type": "integer",
          "examples": [20]
        },
        "fom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-01"]
        },
        "tom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-31"]
        },
        "utbetalingslinjer": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/utbetalingslinje"
          }
        }
      }
    },
    "personOppdrag": {
      "type": ["object", "null"],
      "additionalProperties": false,
      "description": "Optional. Oppdraget som gjelder utbetaling til sykmeldt.",
      "required": [
        "mottaker",
        "fagområde",
        "fagsystemId",
        "nettoBeløp",
        "stønadsdager",
        "fom",
        "tom",
        "utbetalingslinjer"
      ],
      "properties": {
        "mottaker": {
          "type": "string",
          "description": "Sykmeldt"
        },
        "fagområde": {
          "type": "string",
          "enum": ["SP"]
        },
        "fagsystemId": {
          "type": "string",
          "examples": ["L52NYV4KE5BEPILU4L2ERGAVYU"]
        },
        "nettoBeløp": {
          "type": "integer",
          "examples": [7155]
        },
        "stønadsdager": {
          "type": "integer",
          "examples": [20]
        },
        "fom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-01"]
        },
        "tom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-31"]
        },
        "utbetalingslinjer": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/utbetalingslinje"
          }
        }
      }
    }
  },
  "definitions": {
    "utbetalingslinje": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "fom",
        "tom",
        "dagsats",
        "totalbeløp",
        "grad",
        "stønadsdager"
      ],
      "properties": {
        "fom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-01"]
        },
        "tom": {
          "type": "string",
          "format": "date",
          "examples": ["2018-01-31"]
        },
        "dagsats": {
          "type": "integer",
          "examples": [1431]
        },
        "totalbeløp": {
          "type": "integer",
          "examples": [14310]
        },
        "grad": {
          "type": "number",
          "examples": [50.5]
        },
        "stønadsdager": {
          "type": "integer",
          "examples": [20]
        }
      }
    }
  }
}
